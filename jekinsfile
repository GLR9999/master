parallel
pipeline { 
    agent any 
    stages {
        stage('stages Runing in parallel') {
        failFast true
        parallel {
            stage('stage1') {
                steps {
                    echo "stage1"
                    sleep 10
                  }
                }
                stage('stage2') {
                   steps {
                     echo "stage2"
                     sleep 10
                  }
                }
            }
        }
    }
}
